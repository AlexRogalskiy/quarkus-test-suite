{
  "realm": "test-realm",
  "enabled": true,
  "sslRequired": "none",
  "ssoSessionMaxLifespan": 5,
  "roles": {
    "realm": [
      {
        "name": "test-user-role"
      },
      {
        "name": "test-admin-role"
      }
    ]
  },
  "users": [
    {
      "username": "test-user",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "test-user"
        }
      ],
      "realmRoles": [
        "test-user-role"
      ]
    },
    {
      "username": "test-admin",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "test-admin"
        }
      ],
      "realmRoles": [
        "test-user-role",
        "test-admin-role"
      ]
    }
  ],
  "clients": [
    {
      "clientId": "test-application-client",
      "enabled": true,
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "test-application-client-secret",
      "redirectUris": [
        "*"
      ]
    }
  ]
}
